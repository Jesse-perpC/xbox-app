<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="es" lang="es">
<head>
    <title>Acceso Restringido</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">


    <style type="text/css"> @import "dijit/themes/claro/claro.css";

    @import "/javascript/dojo/resources/dojo.css" </style>

    <script type="text/javascript" src="dojo/dojo.js" djConfig="parseOnLoad: true"></script>

    <script> require(["dijit/Fieldset", "dojo/dom", "dojo/domReady!"], function (Fieldset, dom) {
        var tp = new Fieldset({title: "I'm a Fieldset", content: "Collapse me!"});
        dom.byId("holder").appendChild(tp.domNode);
        tp.startup();
    }); </script>

    <script> require(["dijit/form/Button", "dojo/dom", "dojo/domReady!"], function (Button, dom) {
        //Create a button programmatically:
        var myButton = new Button({
                    label: "Click me!",
                    onClick: function(){
                        // Do something: dom.byId("result1").innerHTML += "Thank you! ";
                    }
                }, "progButtonNode").startup(); });
    </script>

</head>
<body class="claro" style="margin-left: 40%; margin-right: 35%; margin-top: 20%;">

<div id="holder"></div>
<button id="progButtonNode" type="button"></button>
<div id="result1"></div>

</body>
</html>